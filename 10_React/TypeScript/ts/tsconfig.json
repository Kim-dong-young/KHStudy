{
  "compilerOptions": {
    "target": "ESNext", // 자바스크립트 버전 설정 -> ESNext(최신버전)
    "module": "NodeNext", // 변환되는 자바스크립트 모듈 시스템 문법 설정 CommonJS / NodeNext(최신버전)
    "outDir": "dist", // 컴파일 결과 생성할 자바스크립트를 저장할 위치 설정 옵션
    "strict": true, // 타입 검사시 엄격함의 수준을 정하는 옵션( true(무조건 변수에 타입지정)/false )
    "moduleDetection": "force", // 기본적으로 파일에 만드는 변수를 로컬데이터로 사용한다. 안주면 변수명 겹치면 사용불가
  },
  "include": ["src"] // 컴파일할 범위를 지정한다. ex) src -> src폴더 내 모든 ts파일
}
